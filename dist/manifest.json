{
  "manifest_version": 3,
  "name": "Amazon Utils",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": ["https://*.amazon.co.uk/*"],
      "js": ["content-script.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Filter products"
  }
}
